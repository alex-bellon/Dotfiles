!rofi.color-window: #151515, #f18b6d, #f18b6d
!rofi.color-normal: #151515, #bbbbbb, #151515, #f18b6d, #ffffff
!rofi.color-active: #f18b6d, #ffffff, #f18b6d, #f18b6d, #ffffff
!rofi.color-urgent: #f18b6d, #ffffff, #f18b6d, #f18b6d, #ffffff
rofi.width: 50
